<div class="table">
  <table>
    <thead>
      <tr>
        <th>First Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Birth Date</th>
        <th>Gender</th>
        <th>Subject</th>
        <th colspan="3">Actions</th>
        <td><%= link_to 'New', new_register_path ,class:'btn btn-register'%></td>
      </tr>
    </thead>
    <tbody>
      <% @register.each do |user| %>
        <tr>
          <td><%= user.firstName %></td>
          <td><%= user.email %></td>
          <td><%= user.phone %></td>
          <td><%= user.birthDate %></td>
          <td><%= user.gender %></td>
          <td><%= user.selectSubject %></td>
          <td><%= button_to 'Destroy', register_path(user), method: :delete, class: 'btn btn-danger' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
